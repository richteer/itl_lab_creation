URxvt*background: #151515
URxvt*foreground: #aeaeae
URxvt*scrollBar: False
URxvt*termName: rxvt-256color

URxvt.urlLauncher : firefox
URxvt.underlineURLs: true
URxvt.urlButton: 1


!URxvt.transparent:	true
!URxvt.shading:	true


!general dialog box (affects main hostname, username, password text)
xscreensaver.Dialog.foreground:         #E6E6E6
xscreensaver.Dialog.background:         #3C3C3C
xscreensaver.Dialog.topShadowColor:     #3C3C3C
xscreensaver.Dialog.bottomShadowColor:  #3C3C3C
xscreensaver.Dialog.Button.foreground:  #E6E6E6
xscreensaver.Dialog.Button.background:  #2E2E2E
!username/password input box and date text colour
xscreensaver.Dialog.text.foreground:    #E6E6E6
xscreensaver.Dialog.text.background:    #2E2E2E
xscreensaver.Dialog.internalBorderWidth:1
xscreensaver.Dialog.borderWidth:        1
xscreensaver.Dialog.shadowThickness:    2
!timeout bar (background is actually determined by Dialog.text.background)
xscreensaver.passwd.thermometer.foreground:  #E6E6E6
xscreensaver.passwd.thermometer.background:  #2E2E2E
xscreensaver.passwd.thermometer.width:       8
